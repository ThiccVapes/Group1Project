<p id="notice"><%= notice %></p>

<h1>Listing Plays</h1>

<table>
  <thead>
    <tr>
      <th>Playid</th>
      <th>Userid</th>
      <th>Time played</th>
      <th>Start play</th>
      <th>End play</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plays.each do |play| %>
      <tr>
        <td><%= play.playID %></td>
        <td><%= play.userID %></td>
        <td><%= play.time_played %></td>
        <td><%= play.start_play %></td>
        <td><%= play.end_play %></td>
        <td><%= link_to 'Show', play %></td>
        <td><%= link_to 'Edit', edit_play_path(play) %></td>
        <td><%= link_to 'Destroy', play, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Play', new_play_path %>
